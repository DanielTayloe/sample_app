var availableTags
$(function(){
    availableTags = <%= Micropost.all.map{ |i| i.content} %>
    $( "#tags" ).autocomplete({
            source: availableTags
    });
    $("#tags").keydown(function(){
        availableTags = <%= Micropost.all.map{ |i| i.content} %>
        $( "#tags" ).autocomplete({
            source: availableTags
        })
    })
});