$(function() {
    var microsearch = <%= Micropost.all.map{ |i| i.content} %>;
    $( "#microsearch_tag" ).autocomplete({
        source: microsearch
    });
});

$(function() {
    var usersearch = <%= User.all.map{ |i| i.name} %>;
    $( "#usersearch_tag" ).autocomplete({
        source: usersearch
    });
});